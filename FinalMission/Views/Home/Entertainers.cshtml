@model List<Entertainer>

<div class="container">
    <div class="row">
        <div class="col">
            <div class="fw-bold">Stage Name</div>
        </div>
        <div class="col">
            <!-- Empty column for spacing purposes -->
        </div>
    </div>

    @foreach (var x in Model)
    {
        <div class="row">
            <div class="col">
                @x.EntStageName
            </div>
            <div class="col">
                <a class="btn btn-info" asp-controller="Home" asp-action="Details" asp-route-id="@x.EntertainerId">Details</a>
            </div>
        </div>
    }
    <br />

    <div class="row">
        <div class="col">
            <!-- Empty column for spacing purposes -->
        </div>
        <div class="col">
            <a class="btn btn-info" asp-controller="Home" asp-action="AddEnt">Add Entertainer</a>
        </div>
    </div>
</div>
